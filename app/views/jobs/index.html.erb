<h1 class="text-center m-3">Jobs Page</h1>
<%= link_to 'Create a Job Lsiting', new_job_path, class: "btn btn-primary"%>

<div class="jobs-cards">
  <% @jobs.each do |job| %>
    <%= link_to job_path(job) do %>
      <div class="content">
        <h5><%= job.profession %></h5>
        <p><%= job.user.email %></p>
      </div>
    <% end %>
  <% end %>
</div>
